
@inherits Panel

<root>
    @foreach( var client in Game.Clients )
    {
        <PlayerListEntry Client=@client />
    }
</root>

@code {
    protected override int BuildHash()
    {
        var result = 0;

        foreach (var client in Game.Clients)
        {
            if (client.Pawn is not StrafePlayer pl) 
                continue;
            result = HashCode.Combine(result, client.Ping);
        }
        return result;
    }
}
