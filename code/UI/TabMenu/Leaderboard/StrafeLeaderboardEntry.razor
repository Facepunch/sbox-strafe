
@inherits Panel;

<root>
    @if(Entry != null)
    {
        <panel>
            <label class="rank" text="@Entry.Rank" />
            <label class="name" text="@Entry.PlayerName" />
        </panel>
        <label text="@Entry.Time.ToTime()" />
    }
</root>

@code {
    internal PersonalBestEntry Entry { get; set; }
}
